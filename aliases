# nav
alias ..="cd .."
alias ...="cd ../.."

# ls
alias l="ls --color --group-directories-first"
alias ll="ls -lF --color --group-directories-first"
alias la="ls -laF --color --group-directories-first"
alias ls="command ls --color --group-directories-first"

# unix
alias e="$EDITOR"

# git
alias g="git"

# bundler
alias b="bundle"
alias be="bundle exec"

# heroku
alias h="heroku"
alias hlog="heroku logs -t"

# rails
alias r="rails"
