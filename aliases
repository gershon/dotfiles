# nav
alias ..="cd .."
alias ...="cd ../.."
alias W="cd ~/Work/"
alias D="cd ~/Downloads"
alias h="history"

# ls
alias l="ls --color"
alias ll="ls -lF --color"
alias la="ls -laF --color"
alias ls="command ls --color"

# unix
alias e="$EDITOR"

# git
alias g="git"

# bundle
alias b="bundle"
alias be="bundle exec"

# Heroku
alias h="heroku"
alias hlog="heroku logs -t"

# Rails
alias r="rails"
alias f="foreman"
alias devlog="tail -f log/development.log"

# Google
chrome="google-chrome"

# IP
alias ips="ifconfig | ruby -ne 'puts $1 if /dr:(\S+)/'"
