# Emacs
alias e="emacs -nw"
alias econfig="e ~/.emacs"

# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias W="~/Work/"

# Edit configs
alias zshconfig="e ~/.zshrc"
alias ohmyzshconfih="e ~/.oh-my-zsh"
alias aliasconfig="e ~/.aliases"
alias zshsource="source ~/.zshrc"

# Git
alias g="git"
alias gconfig="e ~/.gitconfig"
alias st="status"

# Rubygem
alias gemconfig="e ~/.gemrc"
alias be="bundle exec"

# Heroku
alias h="heroku"
alias hpush="g push heroku master"
alias hpull="g pull heroku master"

# Rails
alias r="rails"
alias fs="foreman start"
alias devlog="tail -f log/development.log"

# Google
chrome="google-chrome"

# IP
alias ips="ifconfig | ruby -ne 'puts $1 if /dr:(\S+)/'"

# Tar
alias untar="tar -xvf"
